{
  "name": "mocktrello",
  "version": "0.1.0",
  "homepage": "https://balaSpyrus.github.io/mocktrello",
  "private": true,
  "dependencies": {
    "firebase": "^10.12.1",
    "lodash": "^4.17.21",
    "react": "^16.14.0",
    "react-app-polyfill": "^1.0.6",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^16.14.0",
    "react-icons": "^4.10.1",
    "react-modal": "^3.16.1",
    "react-scripts": "4.0.3",
    "styled-components": "^6.0.7",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "prepare": "husky install",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint --cache --fix 'src/**/*.{js,jsx,ts,tsx,json}'",
    "format": "prettier --write --ignore-unknown 'src/**/*.{js,jsx,ts,tsx,css,md,json}' --config ./.prettierrc"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ],
    "src/**/*.{js,jsx,ts,tsx,css,md,json}": [
      "npm run format"
    ]
  },
  "resolutions": {
    "react-error-overlay": "6.0.9"
  },
  "browserslist": {
    "production": [
      "ie 11",
      "ie 9-11",
      "ie > 9",
      "last 100 ie version",
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "ie 11",
      "ie 9-11",
      "ie > 9",
      "last 100 ie version",
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11",
    "@types/jest": "^29.5.4",
    "@types/lodash": "^4.14.197",
    "@types/node": "^20.5.6",
    "@types/react": "^18.2.21",
    "@types/react-beautiful-dnd": "^13.1.4",
    "@types/react-custom-scrollbars": "^4.0.10",
    "@types/react-dom": "^18.2.7",
    "@types/react-icons": "^3.0.0",
    "@types/react-modal": "^3.16.0",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "2.8.8",
    "react-error-overlay": "^6.0.9"
  }
}
